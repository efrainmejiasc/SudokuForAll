@model SudokuForAll.Models.Sistema.Respuesta


<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
<link href="~/Content/css/Global.css" rel="stylesheet" />
<script src="~/Scripts/jquery-3.3.1.min.js" type="text/javascript"></script>
<script src="~/Content/js/Global.js" type="text/javascript"></script>

@{ 
    ViewBag.Title = "Contact";
}
<h3>Bienvenido  a sudoku para todos</h3>

<div align="center" class="divEmail">
    @using (Html.BeginForm("Contact", "Home"))
    {
        <h4 class="text-primary">Sudoku para todos</h4>
        <input type="email" id="email" name="email" placeholder="Ingrese email" required class="form-control" style="text-align:center;" />
        <div style="height:6px;"></div>
        <input type="submit" id="sendMail" name="sendMail" value="Enviar" class="btn btn-primary" style="width:35%;" />
    }
</div>

<script>
    OcultarObjeto('entrar');
    OcultarObjeto('lenguaje');
</script>
